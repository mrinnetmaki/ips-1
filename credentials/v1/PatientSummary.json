{
  "vct": "https://ips.todiste.fi/credentials/v1/PatientSummary",
  "PatientSummary_SD_JWT_VC": {
    "format": "dc+sd-jwt",
    "cryptographic_binding_methods_supported": ["did:web", "did:jwk"],
    "cryptographic_suites_supported": ["ES256"],
    "credential_signing_alg_values_supported": ["ES256"],
    "proof_types_supported": {
      "jwt": {
        "proof_signing_alg_values_supported": ["ES256"]
      }
    },
    "display": [
      {
        "locale": "en",
        "name": "Patient Summary",
        "description": "International Patient Summary composition",
        "background_color": "#87c9fb",
        "text_color": "#FFFFFF",
        "logo": {
          "uri": "https://ips.todiste.fi/img/ips.png",
          "alt_text": "IPS"
        },
        "rendering": {
          "simple": {
            "text_color": "#FFFFFF",
            "background_color": "#87c9fb",
            "logo": {
              "uri": "https://ips.todiste.fi/img/ips.png",
              "alt_text": "IPS"
            }
          }
        }
      }
    ],
    "claims": [
      {
        "path": ["Text"],
        "display": [
          {
            "lang": "en",
            "locale": "en",
            "label": "Text",
            "description": "Text summary of the resource, for human interpretation"
          }
        ],
        "mandatory": false
      },
      {
        "path": ["Identifier"],
        "display": [
          {
            "lang": "en",
            "locale": "en",
            "label": "Identifier",
            "description": "Version-independent identifier for the Composition"
          }
        ],
        "mandatory": false
      },
      {
        "path": ["Subject"],
        "display": [
          {
            "lang": "en",
            "locale": "en",
            "label": "Subject",
            "description": "Who and/or what the composition is about"
          }
        ],
        "mandatory": false
      },
      {
        "path": ["Date"],
        "display": [
          {
            "lang": "en",
            "locale": "en",
            "label": "Date",
            "description": "Composition editing time"
          }
        ],
        "mandatory": false
      },
      {
        "path": ["Author"],
        "display": [
          {
            "lang": "en",
            "locale": "en",
            "label": "Author",
            "description": "Who and/or what authored the IPS"
          }
        ],
        "mandatory": false
      },
      {
        "path": ["Title"],
